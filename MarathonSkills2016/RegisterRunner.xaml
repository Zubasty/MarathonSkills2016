<Window x:Class="MarathonSkills2016.RegisterRunner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarathonSkills2016"
        mc:Ignorable="d"
        Title="RegisterRunner" Height="450" Width="800">
    <Grid>
        <Button x:Name="IOld_B" Content="Я участвовал ранее" HorizontalAlignment="Left" Margin="228,85,0,0" VerticalAlignment="Top" Width="296" Height="111" Click="Login_B_Click"/>
        <Button x:Name="INew_B" Content="Я новый участник" HorizontalAlignment="Left" Margin="228,227,0,0" VerticalAlignment="Top" Width="296" Height="111" Click="INew_B_Click"/>
        <Button x:Name="Login_B" Content="Login" HorizontalAlignment="Left" Margin="640,346,0,0" VerticalAlignment="Top" Width="118" Height="45" Click="Login_B_Click"/>
        <Button x:Name="Back_B" Content="Назад" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="Back_B_Click"/>

    </Grid>
</Window>
