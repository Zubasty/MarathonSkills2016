<Window x:Class="MarathonSkills2016.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarathonSkills2016"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Email_TB" HorizontalAlignment="Left" Height="23" Margin="346,129,0,0" TextWrapping="Wrap" Text="Email_TB" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password_TB" HorizontalAlignment="Left" Height="23" Margin="346,191,0,0" TextWrapping="Wrap" Text="Password_TB" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Login_B" Content="Login" HorizontalAlignment="Left" Margin="264,253,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Cancel_B" Content="Cancel" HorizontalAlignment="Left" Margin="369,253,0,0" VerticalAlignment="Top" Width="75" Click="Back_B_Click"/>
        <Button x:Name="Back_B" Content="Назад" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="Back_B_Click"/>

    </Grid>
</Window>
